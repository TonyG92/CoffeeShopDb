@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Quantity</th>
        <th>Price</th>
        <th></th>
        <th></th>
    </tr>
    foreach(var shop in ViewBag.ShopList)
    {
    <tr>
        <td>shop.Name</td>
        <td>shop.Description</td>
        <td>shop.Quantity</td>
        <td>shop.Price</td>
        <td><a href="../Shop/UpdateShop?shop.Id" class="btn-default"> Update Shop</a></td>

    </tr>
    }



</table>
